<GridPane alignment="center" hgap="10" stylesheets="@LoginCss.css" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.LoginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="200.0" prefWidth="300.0">
         <children>
            <Button fx:id="loginButton" layoutX="215.0" layoutY="156.0" mnemonicParsing="false" onAction="#login_alert" text="Login" />
            <Label layoutX="41.0" layoutY="68.0" text="ID" />
            <TextField fx:id="idText" layoutX="99.0" layoutY="63.0" prefHeight="30.0" prefWidth="186.0" />
            <Label layoutX="15.0" layoutY="110.0" text="Password" />
            <PasswordField fx:id="passwordText" layoutX="99.0" layoutY="110.0" prefHeight="30.0" prefWidth="186.0" />
            <Label layoutX="66.0" layoutY="23.0" text="Let's Borrow Classroom" textFill="#8d5858">
               <font>
                  <Font name="Microsoft YaHei Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</GridPane>
